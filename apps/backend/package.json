{
  "name": "@no-name-store/backend",
  "version": "1.0.0",
  "description": "Backend API for NoName Store - Clean Architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "backend",
    "api",
    "clean-architecture",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^6",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/sequelize": "^6.12.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@no-name-store/domain": "workspace:*",
    "@types/express": "^5.0.3",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "mysql2": "^3.14.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  }
}
